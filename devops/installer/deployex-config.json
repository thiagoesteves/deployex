{
    "app_name": "myapp",
    "app_lang": "elixir",
    "replicas": 3,
    "account_name": "prod",
    "deployex_hostname": "deployex.example.com",
    "release_adapter": "s3",
    "release_bucket": "myapp-prod-distribution",
    "secrets_adapter": "aws",
    "secrets_path": "deployex-myapp-prod-secrets",
    "aws_region": "sa-east-1",
    "google_credentials": "/home/ubuntu/gcp-config.json",
    "version": "0.3.0-rc11",
    "os_target": "ubuntu-20.04", // "ubuntu-20.04" or "ubuntu-22.04"
    "deploy_timeout_rollback_ms": 600000,
    "deploy_schedule_interval_ms": 5000,
    "env": {
        "MYAPP_PHX_HOST": "example.com",
        "MYAPP_PHX_SERVER": "true",
        "MYAPP_CLOUD_ENVIRONMENT": "prod",
        "MYAPP_OTP_TLS_CERT_PATH": "/usr/local/share/ca-certificates"
    }
  }